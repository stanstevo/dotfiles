! terminal

*.scrollBar: false
Xcursor.theme: cursor-theme

URxvt.cursorUnderline: true
URxvt.letterSpace: 0

#define font1         Inconsolata LGC
#define font2         Inconsolata Nerd Font
#define font3         DejaVu Sans Mono Nerd Font
#define font4         Noto Sans Mono CJK JP

URxvt*letterSpace: 0
URxvt*font:           xft:font1:pixelsize=10, xft:font2:pixelsize=10:minspace=false, xft:font3:pixelsize=10,``xft:font4:pixelsize=10
URxvt*boldFont:       xft:font1:bold:pixelsize=10, xft:font2:pixelsize=10:minspace=False,xft:font3:pixelsize=10,xft:font4:pixelsize=10
URxvt*italicFont:     xft:font1:italic:pixelsize=10, xft:font2:pixelsize=10:minspace=False,xft:font3:pixelsize=10,xft:font4:pixelsize=10
URxvt*boldItalicFont: xft:font1:bold:italic:pixelsize=10, xft:font2:pixelsize=10:minspace=False, xft:font3:pixelsize=10, xft:font4:pixelsize=10
XTerm*faceName: JetBrainsMono Nerd Font:size=10:antialias=true

! X colors.
! Generated by 'wal'
*foreground:        #fff6c9
*background:        #0b1117
*.foreground:       #fff6c9
*.background:       #0b1117
emacs*foreground:   #fff6c9
emacs*background:   #0b1117
URxvt*foreground:   #fff6c9
XTerm*foreground:   #fff6c9
UXTerm*foreground:  #fff6c9
URxvt*background:   [100]#0b1117
XTerm*background:   #0b1117
UXTerm*background:  #0b1117
URxvt*cursorColor:  #fff6c9
XTerm*cursorColor:  #fff6c9
UXTerm*cursorColor: #fff6c9
URxvt*borderColor:  [100]#0b1117

! Colors 0-15.
*.color0: #0b1117
*color0:  #0b1117
*.color1: #ED1A2A
*color1:  #ED1A2A
*.color2: #359B64
*color2:  #359B64
*.color3: #EB6449
*color3:  #EB6449
*.color4: #5A4D43
*color4:  #5A4D43
*.color5: #AF4844
*color5:  #AF4844
*.color6: #96282F
*color6:  #96282F
*.color7: #efbf9c
*color7:  #efbf9c
*.color8: #1a2939
*color8:  #1a2939
*.color9: #ff1027
*color9:  #ff1027
*.color10: #37d780
*color10:  #37d780
*.color11: #ff744a
*color11:  #ff744a
*.color12: #7f624c
*color12:  #7f624c
*.color13: #f34e48
*color13:  #f34e48
*.color14: #cf2732
*color14:  #cf2732
*.color15: #fff6c9
*color15:  #fff6c9

! Black color that will not be affected by bold highlighting.
*.color66: #0b1117
*color66:  #0b1117

! Xclock colors.
XClock*foreground: #fff6c9
XClock*background: #0b1117
XClock*majorColor:  rgba:ff/f6/c9/ff
XClock*minorColor:  rgba:ff/f6/c9/ff
XClock*hourColor:   rgba:ff/f6/c9/ff
XClock*minuteColor: rgba:ff/f6/c9/ff
XClock*secondColor: rgba:ff/f6/c9/ff

! Set depth to make transparency work.
URxvt*depth: 32
